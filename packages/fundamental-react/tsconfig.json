{
    "compilerOptions": {
      "target": "es5",
      "allowJs": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
      "outDir":"es",
      "rootDir":"src",
      "noUnusedLocals": true
    },
    "include": ["src"],
    "exclude":["**/*.test.js", "es","node_modules"]
  }